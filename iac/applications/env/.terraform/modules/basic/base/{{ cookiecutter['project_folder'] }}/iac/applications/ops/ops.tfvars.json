{
  "api_services": {
    "artifactregistry": "artifactregistry.googleapis.com",
    "billing": "cloudbilling.googleapis.com",
    "cloudbuild": "cloudbuild.googleapis.com",
    "cloudresourcemanager": "cloudresourcemanager.googleapis.com",
    "iam": "iam.googleapis.com",
    "iamcredentials": "iamcredentials.googleapis.com",
    "iap": "iap.googleapis.com",
    "servicecontrol": "servicecontrol.googleapis.com",
    "servicemanagement": "servicemanagement.googleapis.com"
  },
  "artifact_registry_repositories": {
    "pipeline-containers": {
      "description": "Repository containing pipeline Docker containers.",
      "format": "DOCKER",
      "location": "{{ cookiecutter['ar_region'] }}",
      "role_group_map": {}
    },
    "pipeline-packages": {
      "description": "Repository containing pipeline Python packages.",
      "format": "PYTHON",
      "location": "{{ cookiecutter['ar_region'] }}",
      "role_group_map": {}
    },
    "pipeline-templates": {
      "description": "Repository containing Kubeflow Pipelines templates.",
      "format": "KFP",
      "location": "{{ cookiecutter['ar_region'] }}",
      "role_group_map": {}
    }
  },
  "component_triggers": {},
  "env_projects": [
    "{{ cookiecutter['project_id_dev'] }}",
    "{{ cookiecutter['project_id_uat'] }}",
    "{{ cookiecutter['project_id_prod'] }}"
  ],
  {% if cookiecutter['include_iam'] == "Yes" %}"folders": {},
  "group_roles": {},
  "groups": {},
  "include_iam": true,
  {% else %}"include_iam": false,
  {% endif %}"pipeline_triggers": {},
  "project_id": "{{ cookiecutter['project_id_ops'] }}",
  "region": "{{ cookiecutter['region'] }}",
  "repo_name": "{{ cookiecutter['repo_name'] }}",
  "repo_owner": "{{ cookiecutter['repo_owner'] }}",
  {% if cookiecutter['include_iam'] == "Yes" %}"service_account_roles": {
    "cloudbuild": [
      "roles/cloudbuild.builds.builder"
    ],
    "terraform": [
      "roles/owner"
    ]
  },
  "service_accounts": {
    "cloudbuild": {
      "create": false
    },
    "terraform": {
      "create": false,
      "email": "{{ cookiecutter['tf_sa_ops'] }}"
    }
  },
  "user_roles": {},
  {% else %}"service_accounts": {
    "terraform": {
      "create": false,
      "email": "{{ cookiecutter['tf_sa_ops'] }}"
    }
  },
  {% endif %}"zone": "{{ cookiecutter['gcp_zone'] }}"
}
