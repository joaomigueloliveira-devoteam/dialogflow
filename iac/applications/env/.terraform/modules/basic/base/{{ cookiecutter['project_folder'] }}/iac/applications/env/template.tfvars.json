{
  "api_services": {
    "aiplatform": "aiplatform.googleapis.com",
    "artifactregistry": "artifactregistry.googleapis.com",
    "automl": "automl.googleapis.com",
    "bigquery": "bigquery.googleapis.com",
    "billing": "cloudbilling.googleapis.com",
    "cloudbuild": "cloudbuild.googleapis.com",
    "cloudresourcemanager": "cloudresourcemanager.googleapis.com",
    "compute": "compute.googleapis.com",
    "iam": "iam.googleapis.com",
    "iamcredentials": "iamcredentials.googleapis.com",
    "iap": "iap.googleapis.com",
    "ml": "ml.googleapis.com",
    "run": "run.googleapis.com",
    "servicecontrol": "servicecontrol.googleapis.com",
    "servicemanagement": "servicemanagement.googleapis.com"
  },
  "buckets": {
    "datasets": {
      "name": "{% raw %}{{ gcp_project_id }}-datasets{% endraw %}",
      "region": "{{ cookiecutter['region'] }}"
    },
    "models": {
      "name": "{% raw %}{{ gcp_project_id }}-models{% endraw %}",
      "region": "{{ cookiecutter['region'] }}"
    }
  },
  "environment": "{% raw %}{{ env }}{% endraw %}",
  {% if cookiecutter['include_iam'] == "Yes" %}"folders": {},
  "group_roles": {},
  "groups": {},
  "include_iam": true,
  {% else %}"include_iam": false,
  {% endif %}"project_id": "{% raw %}{{ gcp_project_id }}{% endraw %}",
  "region": "{{ cookiecutter['region'] }}",
  {% if cookiecutter['include_iam'] == "Yes" %}"service_account_roles": {
    "cloudbuild": [
      "roles/cloudbuild.builds.builder"
    ],
    "terraform": [
      "roles/owner"
    ],
    "terraform_ops": [
      "roles/viewer"
    ]
  },
  "service_accounts": {
    "cloudbuild": {
      "create": false
    },
    "terraform": {
      "create": false,
      "email": "{% raw %}{{ terraform_service_account }}{% endraw %}"
    },
    "terraform_ops": {
      "create": false,
      "email": "{{ cookiecutter['tf_sa_ops'] }}"
    }
  },
  "user_roles": {},
  {% else %}"service_accounts": {
    "terraform": {
      "create": false,
      "email": "{% raw %}{{ terraform_service_account }}{% endraw %}"
    }
  },
  {% endif %}"zone": "{{ cookiecutter['gcp_zone'] }}"
}
